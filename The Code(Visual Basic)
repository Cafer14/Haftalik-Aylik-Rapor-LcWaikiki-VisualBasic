Range("G67") = "%" & Round((100 * (Range("F67"))) / (Range("E67") + 0.00000001), 0) 'indirimli stok oranı
Range("J50") = "%" & Round((100 * (Range("I50"))) / (Range("F50") + Range("E50") + 0.00000001), 0) 'bg aks indirimli stok oranı
Range("J51") = "%" & Round((100 * (Range("I51"))) / (Range("F51") + Range("E51") + 0.00000001), 0) 'bu aks indirimli stok oranı
Range("J52") = "%" & Round((100 * (Range("I52"))) / (Range("F52") + Range("E52") + 0.00000001), 0) 'ck aks stok oranı
