'ÇEKİLEN ÜRÜN
Do While (Sayfa5.Cells(d, 1) > 0)
    If (Left(Sayfa5.Cells(d, 1), 2) = "BG") Then
        If (Left(Sayfa5.Cells(d, 2), 3) = "BGA") Then
            dce = Sayfa5.Cells(d, 3) 'bga çekilen
            dcf = dcf + dce 'bga çekilen toplam
            d = d + 1
        ElseIf (Left(Sayfa5.Cells(d, 2), 3) = "BGK") Then
            ace = Sayfa5.Cells(d, 3) 'bgk çekilen
            acf = acf + ace 'bgk çekilen toplam
            d = d + 1
        ElseIf (Left(Sayfa5.Cells(d, 2), 3) = "BGR") Then
            cce = Sayfa5.Cells(d, 3) 'bgr çekilen
            ccf = ccf + cce 'bgr çekilen toplam
            d = d + 1
        ElseIf (Left(Sayfa5.Cells(d, 2), 3) = "BGD") Then
            bce = Sayfa5.Cells(d, 3) 'bgd çekilen
            bcf = bcf + bce 'bgd çekilen toplam
            d = d + 1
        Else
            fce = Sayfa5.Cells(d, 3) 'bg diğer çekilen
            fcf = fcf + fce 'bg diğer çekilen toplamı
            d = d + 1
        End If
    ElseIf (Left(Sayfa5.Cells(d, 1), 2) = "BU") Then
        If (Left(Sayfa5.Cells(d, 2), 3) = "BUA") Then
            ice = Sayfa5.Cells(d, 3) 'bua çekilen
            icf = icf + ice 'bua çekilen toplam
            d = d + 1
        ElseIf (Left(Sayfa5.Cells(d, 2), 3) = "BUK") Then
            gce = Sayfa5.Cells(d, 3) 'buk çekilen
            gcf = gcf + gce 'buk çekilen toplam
            d = d + 1
        ElseIf (Left(Sayfa5.Cells(d, 2), 3) = "BUR") Then
            ıce = Sayfa5.Cells(d, 3) 'bur çekilen
            ıcf = ıcf + ıce 'bur çekilen toplam
            d = d + 1
        ElseIf (Left(Sayfa5.Cells(d, 2), 3) = "BUD") Then
            hce = Sayfa5.Cells(d, 3) 'bud çekilen
            hcf = hcf + hce 'bud çekilen toplam
            d = d + 1
        Else
            kce = Sayfa5.Cells(d, 3) 'bu diğer çekilen
            kcf = kcf + kce 'bu diğer çekilen toplamı
            d = d + 1
        End If
    ElseIf (Left(Sayfa5.Cells(d, 1), 2) = "CK") Then
        If (Left(Sayfa5.Cells(d, 2), 4) = "CK4A") Then
            oce = Sayfa5.Cells(d, 3) 'ck4a çekilen
            ocf = ocf + oce 'ck4a çekilen toplam
            d = d + 1
        ElseIf (Left(Sayfa5.Cells(d, 2), 4) = "CK1A") Then
            öce = Sayfa5.Cells(d, 3) 'ck1a çekilen
            öcf = öcf + öce 'ck1a çekilen toplam
            d = d + 1
        ElseIf (Left(Sayfa5.Cells(d, 2), 4) = "CK4K" Or Left(Sayfa5.Cells(d, 2), 4) = "CKBK") Then
            lce = Sayfa5.Cells(d, 3) 'ck aks çekilen
            lcf = lcf + lce 'ck aks çekilen toplam
            d = d + 1
        ElseIf (Left(Sayfa5.Cells(d, 2), 4) = "CK4R" Or Left(Sayfa5.Cells(d, 2), 4) = "CKBR") Then
            nce = Sayfa5.Cells(d, 3) 'ck çorap çekilen
            ncf = ncf + nce 'ck çorap çekilen toplam
            d = d + 1
        ElseIf (Left(Sayfa5.Cells(d, 2), 4) = "CK4D") Then
            mce = Sayfa5.Cells(d, 3) 'ck4d çekilen
            mcf = mcf + mce 'ck4d çekilen toplam
            d = d + 1
        Else
            rce = Sayfa5.Cells(d, 3) 'ck diğer çekilen
            rcf = rcf + rce 'ck diğer çekilen toplamı
            d = d + 1
        End If
    ElseIf (Left(Sayfa5.Cells(d, 1), 2) = "CU") Then
        If (Left(Sayfa5.Cells(d, 2), 4) = "CU4A") Then
            uce = Sayfa5.Cells(d, 3) 'cu4a çekilen
            ucf = ucf + uce 'cu4a çekilen toplam
            d = d + 1
        ElseIf (Left(Sayfa5.Cells(d, 2), 4) = "CU1A") Then
            üce = Sayfa5.Cells(d, 3) 'cu1a çekilen
            ücf = ücf + üce 'cu1a çekilen toplam
            d = d + 1
        ElseIf (Left(Sayfa5.Cells(d, 2), 4) = "CU4K" Or Left(Sayfa5.Cells(d, 2), 4) = "CUBK") Then
            sce = Sayfa5.Cells(d, 3) 'cu aks çekilen
            scf = scf + sce 'cu aks çekilen toplam
            d = d + 1
        ElseIf (Left(Sayfa5.Cells(d, 2), 4) = "CU4R" Or Left(Sayfa5.Cells(d, 2), 4) = "CUBR") Then
            tce = Sayfa5.Cells(d, 3) 'cu çorap çekilen
            tcf = tcf + tce 'cu çorap çekilen toplam
            d = d + 1
        ElseIf (Left(Sayfa5.Cells(d, 2), 4) = "CU4D") Then
            şce = Sayfa5.Cells(d, 3) 'cu4d çekilen
            şcf = şcf + şce 'cu4d çekilen toplam
            d = d + 1
        Else
            yce = Sayfa5.Cells(d, 3) 'cu diğer çekilen
            ycf = ycf + yce 'cu diğer çekilen toplamı
            d = d + 1
        End If
    ElseIf (Left(Sayfa5.Cells(d, 1), 2) = "EV") Then
        pce = Sayfa5.Cells(d, 3) 'ev çekilen
        pcf = pcf + pce 'ev çekilen toplamı
        d = d + 1
    Else
        d = d + 1
    End If
Loop
Range("C37") = dcf 'bga çekilen
