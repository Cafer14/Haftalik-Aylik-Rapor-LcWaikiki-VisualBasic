Private Sub Haftalık_Rapor_Click()
Dim a, b, c, d, e As Variant
a = 2
b = 2
c = 2
d = 2
e = 2
' STOK RAPORU
Do While (Sayfa2.Cells(b, 1) > 0)
    If (Sayfa2.Cells(b, 1) = "BG") Then
        If (Sayfa2.Cells(b, 4) = "AKSESUAR" Or Sayfa2.Cells(b, 4) = "BANYO" Or Sayfa2.Cells(b, 4) = "DERİ AKSESUAR" Or Sayfa2.Cells(b, 4) = "DOKUMA AKSESUAR" Or Sayfa2.Cells(b, 4) = "KRAVAT/PAPYON" Or Sayfa2.Cells(b, 4) = "SAÇ AKSESUAR" Or Sayfa2.Cells(b, 4) = "STANDART DIŞI" Or Sayfa2.Cells(b, 4) = "TRİKO ÖRME AKSESUAR") Then
            If (Sayfa2.Cells(b, 3) = "9S    " Or Sayfa2.Cells(b, 3) = "9W    ") Then
                aa = Sayfa2.Cells(b, 5) ' aksesuar yeni sezon depo stok
                ab = ab + aa ' aksesuar yeni sezon depo stok toplamı
                ac = Sayfa2.Cells(b, 6) 'aksesuar yeni sezon reyok stok
                ad = ad + ac 'aksesuar yeni sezon reyon stok toplamı
                b = b + 1
            Else
                ae = Sayfa2.Cells(b, 5) ' aksesuar eski sezon depo stok
