'REYON İNDİRİN ORANLARI
Do While (Sayfa4.Cells(c, 1) > 0)
    If (Sayfa4.Cells(c, 1) = "BG" And Sayfa4.Cells(c, 5) = "KIRMIZI") Then
        If (Sayfa4.Cells(c, 3) = "AKSESUAR" Or Sayfa4.Cells(c, 3) = "BANYO" Or Sayfa4.Cells(c, 3) = "DERİ AKSESUAR" Or Sayfa4.Cells(c, 3) = "DOKUMA AKSESUAR" Or Sayfa4.Cells(c, 3) = "KRAVAT/PAPYON" Or Sayfa4.Cells(c, 3) = "SAÇ AKSESUAR" Or Sayfa4.Cells(c, 3) = "STANDART DIŞI" Or Sayfa4.Cells(c, 3) = "TRİKO ÖRME AKSESUAR") Then
                abe = Sayfa4.Cells(c, 4) ' aksesuar reyon indirim
                abf = abf + abe ' aksesuar reyon indirim toplamı
                c = c + 1
        ElseIf (Sayfa4.Cells(c, 3) = "OYUNCAK") Then
                bbe = Sayfa4.Cells(c, 4) ' oyuncak reyon indirim
                bbf = bbf + bbe ' oyuncak reyon indirim toplamı
                c = c + 1
        ElseIf (Sayfa4.Cells(c, 3) = "ÇORAP") Then
                cbe = Sayfa4.Cells(c, 4) ' çorap reyon indirim
                cbf = cbf + cbe ' çorap reyon indirim toplamı
                c = c + 1
        ElseIf (Sayfa4.Cells(c, 3) = "AYAKKABI") Then
                dbe = Sayfa4.Cells(c, 4) ' ayakkabı reyon indirim
                dbf = dbf + dbe ' ayakkabı reyon indirim toplamı
                c = c + 1
        ElseIf (Sayfa4.Cells(c, 3) = "KOZMETİK" Or Sayfa4.Cells(c, 3) = "TEKSTİL DIŞI") Then
                ebe = Sayfa4.Cells(c, 4) ' kozmetik reyon indirim
                ebf = ebf + ebe ' kozmetik reyon indirim toplamı
                c = c + 1
        Else
                fbe = Sayfa4.Cells(c, 4) ' bg gelen
                fbf = fbf + fbe ' bg reyon indirim toplamı
                c = c + 1
         End If
    ElseIf (Sayfa4.Cells(c, 1) = "BU" And Sayfa4.Cells(c, 5) = "KIRMIZI") Then
          If (Sayfa4.Cells(c, 3) = "AKSESUAR" Or Sayfa4.Cells(c, 3) = "BANYO" Or Sayfa4.Cells(c, 3) = "DERİ AKSESUAR" Or Sayfa4.Cells(c, 3) = "DOKUMA AKSESUAR" Or Sayfa4.Cells(c, 3) = "KRAVAT/PAPYON" Or Sayfa4.Cells(c, 3) = "SAÇ AKSESUAR" Or Sayfa4.Cells(c, 3) = "STANDART DIŞI" Or Sayfa4.Cells(c, 3) = "TRİKO ÖRME AKSESUAR") Then
                gbe = Sayfa4.Cells(c, 4) ' aksesuar indirim
                gbf = gbf + gbe ' aksesuar indirim toplamı
                c = c + 1
        ElseIf (Sayfa4.Cells(c, 3) = "OYUNCAK") Then
                hbe = Sayfa4.Cells(c, 4) ' oyuncak indirim
                hbf = hbf + hbe ' oyuncak indirim toplamı
                c = c + 1
        ElseIf (Sayfa4.Cells(c, 3) = "ÇORAP") Then
                ıbe = Sayfa4.Cells(c, 4) ' çorap indirim
                ıbf = ıbf + ıbe ' çorap indirim toplamı
                c = c + 1
        ElseIf (Sayfa4.Cells(c, 3) = "AYAKKABI") Then
                ibe = Sayfa4.Cells(c, 4) ' ayakkabı indirim
                ibf = ibf + ibe ' ayakkabı indirim toplamı
                c = c + 1
        ElseIf (Sayfa4.Cells(c, 3) = "KOZMETİK" Or Sayfa4.Cells(c, 3) = "TEKSTİL DIŞI") Then
                jbe = Sayfa4.Cells(c, 4) ' kozmetik indirim
                jbf = jbf + jbe ' kozmetik indirim toplamı
                c = c + 1
        Else
                kbe = Sayfa4.Cells(c, 4) ' bu indirim
                kbf = kbf + kbe ' bu indirim toplamı
                c = c + 1
         End If
    ElseIf (Sayfa4.Cells(c, 1) = "CK" And Sayfa4.Cells(c, 5) = "KIRMIZI") Then
          If (Sayfa4.Cells(c, 3) = "AKSESUAR" Or Sayfa4.Cells(c, 3) = "BANYO" Or Sayfa4.Cells(c, 3) = "DERİ AKSESUAR" Or Sayfa4.Cells(c, 3) = "DOKUMA AKSESUAR" Or Sayfa4.Cells(c, 3) = "KRAVAT/PAPYON" Or Sayfa4.Cells(c, 3) = "SAÇ AKSESUAR" Or Sayfa4.Cells(c, 3) = "STANDART DIŞI" Or Sayfa4.Cells(c, 3) = "TRİKO ÖRME AKSESUAR") Then
                lbe = Sayfa4.Cells(c, 4) ' aksesuar indirim
                lbf = lbf + lbe ' aksesuar indirim toplamı
                c = c + 1
        ElseIf (Sayfa4.Cells(c, 3) = "OYUNCAK") Then
                mbe = Sayfa4.Cells(c, 4) ' oyuncak indirim
                mbf = mbf + mbe ' oyuncak indirim toplamı
                c = c + 1
        ElseIf (Sayfa4.Cells(c, 3) = "ÇORAP") Then
                nbe = Sayfa4.Cells(c, 4) ' çorap indirim
                nbf = nbf + nbe ' çorap indirim toplamı
                c = c + 1
        ElseIf (Sayfa4.Cells(c, 3) = "AYAKKABI") Then
                If (Sayfa4.Cells(c, 2) = "CK4A ") Then
                    obe = Sayfa4.Cells(c, 4) ' ayakkabı ck4 indirim
                    obf = obf + obe ' ayakkabı ck4 indirim toplamı
                    c = c + 1
                Else
                    öbe = Sayfa4.Cells(c, 4) ' ayakkabı ck1 indirim
                    öbf = öbf + öbe ' ayakkabı ck1 indirim toplamı
                    c = c + 1
                End If
        ElseIf (Sayfa4.Cells(c, 3) = "KOZMETİK" Or Sayfa4.Cells(c, 3) = "TEKSTİL DIŞI") Then
                pbe = Sayfa4.Cells(c, 4) ' kozmetik indirim
                pbf = pbf + pbe ' kozmetik indirim toplamı
                c = c + 1
        Else
                rbe = Sayfa4.Cells(c, 4) ' ck indirim
                rbf = rbf + rbe ' ck indirim toplamı
                c = c + 1
         End If
    ElseIf (Sayfa4.Cells(c, 1) = "CU" And Sayfa4.Cells(c, 5) = "KIRMIZI") Then
          If (Sayfa4.Cells(c, 3) = "AKSESUAR" Or Sayfa4.Cells(c, 3) = "BANYO" Or Sayfa4.Cells(c, 3) = "DERİ AKSESUAR" Or Sayfa4.Cells(c, 3) = "DOKUMA AKSESUAR" Or Sayfa4.Cells(c, 3) = "KRAVAT/PAPYON" Or Sayfa4.Cells(c, 3) = "SAÇ AKSESUAR" Or Sayfa4.Cells(c, 3) = "STANDART DIŞI" Or Sayfa4.Cells(c, 3) = "TRİKO ÖRME AKSESUAR") Then
                sbe = Sayfa4.Cells(c, 4) ' aksesuar indirim
                sbf = sbf + sbe ' aksesuar indirim toplamı
                c = c + 1
        ElseIf (Sayfa4.Cells(c, 3) = "OYUNCAK") Then
                şbe = Sayfa4.Cells(c, 4) ' oyuncak indirim
                şbf = şbf + şbe ' oyuncak indirim toplamı
                c = c + 1
        ElseIf (Sayfa4.Cells(c, 3) = "ÇORAP") Then
                tbe = Sayfa4.Cells(c, 4) ' çorap indirim
                tbf = tbf + tbe ' çorap indirim toplamı
                c = c + 1
        ElseIf (Sayfa4.Cells(c, 3) = "AYAKKABI") Then
                If (Sayfa4.Cells(c, 2) = "CU4A ") Then
                    ube = Sayfa4.Cells(c, 4) ' ayakkabı ck4 indirim
                    ubf = ubf + ube ' ayakkabı ck4 indirim toplamı
                    c = c + 1
                Else
                    übe = Sayfa4.Cells(c, 4) ' ayakkabı ck1 indirim
                    übf = übf + übe ' ayakkabı ck1 indirim toplamı
                    c = c + 1
                End If
        ElseIf (Sayfa4.Cells(c, 3) = "KOZMETİK" Or Sayfa4.Cells(c, 3) = "TEKSTİL DIŞI") Then
                vke = Sayfa4.Cells(c, 4) ' kozmetik indirim
                vbf = vbf + vke ' kozmetik indirim toplamı
                c = c + 1
        Else
                ybe = Sayfa4.Cells(c, 4) ' cu indirim
                ybf = ybf + ybe ' cu indirim toplamı
                c = c + 1
        End If
    Else
        c = c + 1
    End If
Loop
Range("F60") = fbf 'bg indirim
Range("F61") = kbf 'bu indirim
Range("F62") = rbf 'ck indirim
Range("F63") = ybf 'cu indirim
Range("F64") = cbf + ıbf + nbf + tbf 'çorap indirim
Range("F65") = ebf + jbf + pbf + vbf 'kozmetik indirim
Range("F66") = bbf + hbf + mbf + şbf 'oyuncak indirim
Range("F67") = Range("F60") + Range("F61") + Range("F62") + Range("F63") + Range("F64") + Range("F65") + Range("F66") 'toplam indirim
Range("I37") = dbf 'bg ayakkabı indirim
Range("I38") = ibf 'bu ayakkabı indirim
Range("I39") = obf 'ck4 ayakkabı indirim
Range("I40") = öbf 'ck1 ayakkabı indirim
Range("I41") = ubf 'cu4 ayakkabı indirim
Range("I42") = übf 'cu1 ayakkabı indirim
Range("I43") = Range("I37") + Range("I38") + Range("I39") + Range("I40") + Range("I41") + Range("I42") 'indirim ayakkabı toplamı
