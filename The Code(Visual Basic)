'SATILAN ÜRÜN
Do While (Sayfa6.Cells(e, 2) > 0)
    If (Left(Sayfa6.Cells(e, 1), 2) = "BG") Then
        If (Left(Sayfa6.Cells(e, 2), 3) = "BGA") Then
            dde = Sayfa6.Cells(e, 3) 'bga satılan
            ddf = ddf + dde 'bga satılan toplam
            e = e + 1
        ElseIf (Left(Sayfa6.Cells(e, 2), 3) = "BGK") Then
            ade = Sayfa6.Cells(e, 3) 'bgk satılan
            adf = adf + ade 'bgk satılan toplam
            e = e + 1
        ElseIf (Left(Sayfa6.Cells(e, 2), 3) = "BGR") Then
            cde = Sayfa6.Cells(e, 3) 'bgr satılan
            cdf = cdf + cde 'bgr satılan toplam
            e = e + 1
        ElseIf (Left(Sayfa6.Cells(e, 2), 3) = "BGD") Then
            bde = Sayfa6.Cells(e, 3) 'bgd satılan
            bdf = bdf + bde 'bgd satılan toplam
            e = e + 1
        Else
            fde = Sayfa6.Cells(e, 3) 'bg diğer satılan
            fdf = fdf + fde 'bg diğer satılan toplamı
            e = e + 1
        End If
    ElseIf (Left(Sayfa6.Cells(e, 1), 2) = "BU") Then
        If (Left(Sayfa6.Cells(e, 2), 3) = "BUA") Then
            ide = Sayfa6.Cells(e, 3) 'bua satılan
            idf = idf + ide 'bua satılan toplam
            e = e + 1
        ElseIf (Left(Sayfa6.Cells(e, 2), 3) = "BUK") Then
            gde = Sayfa6.Cells(e, 3) 'buk satılan
            gdf = gdf + gde 'buk satılan toplam
            e = e + 1
        ElseIf (Left(Sayfa6.Cells(e, 2), 3) = "BUR") Then
            ıde = Sayfa6.Cells(e, 3) 'bur satılan
            ıdf = ıdf + ıde 'bur satılan toplam
            e = e + 1
        ElseIf (Left(Sayfa6.Cells(e, 2), 3) = "BUD") Then
            hde = Sayfa6.Cells(e, 3) 'bud satılan
            hdf = hdf + hde 'bud satılan toplam
            e = e + 1
        Else
            kde = Sayfa6.Cells(e, 3) 'bu diğer satılan
            kdf = kdf + kde 'bu diğer satılan toplamı
            e = e + 1
        End If
    ElseIf (Left(Sayfa6.Cells(e, 1), 2) = "CK") Then
        If (Left(Sayfa6.Cells(e, 2), 4) = "CK4A") Then
            ode = Sayfa6.Cells(e, 3) 'ck4a satılan
            odf = odf + ode 'ck4a satılan toplam
            e = e + 1
        ElseIf (Left(Sayfa6.Cells(e, 2), 4) = "CK1A") Then
            öde = Sayfa6.Cells(e, 3) 'ck1a satılan
            ödf = ödf + öde 'ck1a satılan toplam
            e = e + 1
        ElseIf (Left(Sayfa6.Cells(e, 2), 4) = "CK4K" Or Left(Sayfa6.Cells(e, 2), 4) = "CKBK") Then
            lde = Sayfa6.Cells(e, 3) 'ck aks satılan
            ldf = ldf + lde 'ck aks satılan toplam
            e = e + 1
        ElseIf (Left(Sayfa6.Cells(e, 2), 4) = "CK4R" Or Left(Sayfa6.Cells(e, 2), 4) = "CKBR") Then
            nde = Sayfa6.Cells(e, 3) 'ck çorap satılan
            ndf = ndf + nde 'ck çorap satılan toplam
            e = e + 1
        ElseIf (Left(Sayfa6.Cells(e, 2), 4) = "CK4D") Then
            mde = Sayfa6.Cells(e, 3) 'ck4d satılan
            mdf = mdf + mde 'ck4d satılan toplam
            e = e + 1
        Else
            rde = Sayfa6.Cells(e, 3) 'ck diğer satılan
            rdf = rdf + rde 'ck diğer satılan toplamı
            e = e + 1
        End If
    ElseIf (Left(Sayfa6.Cells(e, 1), 2) = "CU") Then
        If (Left(Sayfa6.Cells(e, 2), 4) = "CU4A") Then
            ude = Sayfa6.Cells(e, 3) 'cu4a satılan
            udf = udf + ude 'cu4a satılan toplam
            e = e + 1
        ElseIf (Left(Sayfa6.Cells(e, 2), 4) = "CU1A") Then
            üde = Sayfa6.Cells(e, 3) 'cu1a satılan
            üdf = üdf + üde 'cu1a satılan toplam
            e = e + 1
        ElseIf (Left(Sayfa6.Cells(e, 2), 4) = "CU4K" Or Left(Sayfa6.Cells(e, 2), 4) = "CUBK") Then
            sde = Sayfa6.Cells(e, 3) 'cu aks satılan
            sdf = sdf + sde 'cu aks satılan toplam
            e = e + 1
        ElseIf (Left(Sayfa6.Cells(e, 2), 4) = "CU4R" Or Left(Sayfa6.Cells(e, 2), 4) = "CUBR") Then
            tde = Sayfa6.Cells(e, 3) 'cu çorap satılan
            tdf = tdf + tde 'cu çorap satılan toplam
            e = e + 1
        ElseIf (Left(Sayfa6.Cells(e, 2), 4) = "CU4D") Then
            şde = Sayfa6.Cells(e, 3) 'cu4d satılan
            şdf = şdf + şde 'cu4d satılan toplam
            e = e + 1
        Else
            yde = Sayfa6.Cells(e, 3) 'cu diğer satılan
            ydf = ydf + yde 'cu diğer satılan toplamı
            e = e + 1
        End If
    ElseIf (Left(Sayfa6.Cells(e, 1), 2) = "EV") Then
            pde = Sayfa6.Cells(e, 3) 'ev diğer satılan
            pdf = pdf + pde 'ev diğer satılan toplamı
            e = e + 1
    Else
        e = e + 1
    End If
Loop
Range("D37") = ddf 'bga satılan
Range("D38") = idf 'bua satılan
Range("D39") = odf 'ck4a satılan
Range("D40") = ödf 'ck1a satılan
Range("D41") = udf 'cu4a satılan
Range("D42") = üdf 'cu1a satılan
Range("D43") = Range("D37") + Range("D38") + Range("D39") + Range("D40") + Range("D41") + Range("D42") 'ayk satılan toplamı
Range("D50") = adf 'bg aks satılan
Range("D51") = gdf 'bu aks satılan
Range("D52") = ldf 'ck aks satılan
Range("D53") = sdf 'cu aks satılan
Range("D54") = Range("D50") + Range("D51") + Range("D52") + Range("D53") 'aks satılan toplamı
Range("F7") = fdf 'bg satılan
Range("F8") = kdf 'bu satılan
Range("F9") = rdf 'ck satılan
Range("F10") = ydf 'cu satılan
Range("F11") = pdf 'ev satılan
Range("F12") = cdf + ıdf + ndf + tdf 'çorap satılan
Range("F13") = bdf + hdf + mdf + şdf 'kozmetik-oyuncak satılan
Range("F14") = Range("F7") + Range("F8") + Range("F9") + Range("F10") + Range("F11") + Range("F12") + Range("F13") 'satılan toplam
MsgBox ("İşlem Tamamlandı")
End Sub

