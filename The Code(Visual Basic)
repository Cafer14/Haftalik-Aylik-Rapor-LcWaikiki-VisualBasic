                    vg = Sayfa2.Cells(b, 6) 'ayakkabı eski sezon reyok stok
                    vh = vh + vg 'ayakkabı eski sezon reyon stok toplamı
                    b = b + 1
                End If
            Else
                If (Sayfa2.Cells(b, 3) = "9S    " Or Sayfa2.Cells(b, 3) = "9W    ") Then
                    ya = Sayfa2.Cells(b, 5) ' ayakkabı yeni sezon depo stok
                    yb = yb + ya ' ayakkabı yeni sezon depo stok toplamı
                    yc = Sayfa2.Cells(b, 6) 'ayakkabı yeni sezon reyok stok
                    yd = yd + yc 'ayakkabı yeni sezon reyon stok toplamı
                    b = b + 1
                Else
                    ye = Sayfa2.Cells(b, 5) ' ayakkabı eski sezon depo stok
                    yf = yf + ye ' ayakkabı eski sezon depo stok toplamı
                    yg = Sayfa2.Cells(b, 6) 'ayakkabı eski sezon reyok stok
                    yh = yh + yg 'ayakkabı eski sezon reyon stok toplamı
                    b = b + 1
                End If
            End If
        ElseIf (Sayfa2.Cells(b, 4) = "KOZMETİK" Or Sayfa2.Cells(b, 4) = "TEKSTİL DIŞI") Then
            If (Sayfa2.Cells(b, 3) = "9S    " Or Sayfa2.Cells(b, 3) = "9W    ") Then
                If (Sayfa2.Cells(b, 5) = 0 And Sayfa2.Cells(b, 6) < 0) Then
                    b = b + 1
                Else
                    wa = Sayfa2.Cells(b, 5) ' kozmetik yeni sezon depo stok
                    wb = wb + wa ' kozmetik yeni sezon depo stok toplamı
                    wc = Sayfa2.Cells(b, 6) ' kozmetik yeni sezon reyon stok
                    wd = wd + wc ' kozmetik yeni sezon reyon stok toplamı
                    b = b + 1
                End If
            Else
                If (Sayfa2.Cells(b, 5) = 0 And Sayfa2.Cells(b, 6) < 0) Then
                    b = b + 1
                Else
                    we = Sayfa2.Cells(b, 5) ' kozmetik yeni sezon depo stok
                    wf = wf + we ' kozmetik eski sezon depo stok toplamı
                    wg = Sayfa2.Cells(b, 6) ' kozmetik yeni sezon reyon stok
                    wh = wh + wg ' kozmetik eski sezon reyon stok toplamı
                    b = b + 1
                End If
            End If
        Else
            If (Sayfa2.Cells(b, 3) = "9S    " Or Sayfa2.Cells(b, 3) = "9W    ") Then
                za = Sayfa2.Cells(b, 5) ' diğer(ürün) yeni sezon depo stok
                zb = zb + za ' diğer(ürün yeni sezon depo stok toplamı
                zc = Sayfa2.Cells(b, 6) 'diğer(ürün yeni sezon reyok stok
                zd = zd + zc 'diğer(ürün yeni sezon reyon stok toplamı
                b = b + 1
            Else
                ze = Sayfa2.Cells(b, 5) ' diğer(ürün eski sezon depo stok
                zf = zf + ze ' diğer(ürün eski sezon depo stok toplamı
                zg = Sayfa2.Cells(b, 6) 'diğer(ürün eski sezon reyok stok
                zh = zh + zg 'diğer(ürün eski sezon reyon stok toplamı
                b = b + 1
            End If
         End If
    Else
    b = b + 1
    End If
Loop
