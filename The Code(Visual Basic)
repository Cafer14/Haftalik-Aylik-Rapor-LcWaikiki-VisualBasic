'GELEN SEVKİYAT
Do While (Sayfa3.Cells(a, 1) > 0)
    If (Sayfa3.Cells(a, 1) = "BG") Then
        If (Sayfa3.Cells(a, 4) = "AKSESUAR" Or Sayfa3.Cells(a, 4) = "BANYO" Or Sayfa3.Cells(a, 4) = "DERİ AKSESUAR" Or Sayfa3.Cells(a, 4) = "DOKUMA AKSESUAR" Or Sayfa3.Cells(a, 4) = "KRAVAT/PAPYON" Or Sayfa3.Cells(a, 4) = "SAÇ AKSESUAR" Or Sayfa3.Cells(a, 4) = "STANDART DIŞI" Or Sayfa3.Cells(a, 4) = "TRİKO ÖRME AKSESUAR") Then
                aae = Sayfa3.Cells(a, 3) ' aksesuar gelen
                aaf = aaf + aae ' aksesuar gelen toplamı
                a = a + 1
        ElseIf (Sayfa3.Cells(a, 4) = "OYUNCAK") Then
                bae = Sayfa3.Cells(a, 3) ' oyuncak gelen
                baf = baf + bae ' oyuncak gelen toplamı
                a = a + 1
        ElseIf (Sayfa3.Cells(a, 4) = "ÇORAP") Then
                cae = Sayfa3.Cells(a, 3) ' çorap gelen
                caf = caf + cae ' çorap gelen toplamı
                a = a + 1
        ElseIf (Sayfa3.Cells(a, 4) = "AYAKKABI") Then
                dae = Sayfa3.Cells(a, 3) ' ayakkabı gelen
                daf = daf + dae ' ayakkabı gelen toplamı
                a = a + 1
        ElseIf (Sayfa3.Cells(a, 4) = "KOZMETİK" Or Sayfa3.Cells(a, 4) = "TEKSTİL DIŞI") Then
                eae = Sayfa3.Cells(a, 3) ' kozmetik gelen
                eaf = eaf + eae ' kozmetik gelen toplamı
                a = a + 1
        Else
                fae = Sayfa3.Cells(a, 3) ' bg gelen
                faf = faf + fae ' bg gelen toplamı
                a = a + 1
         End If
    ElseIf (Sayfa3.Cells(a, 1) = "BU") Then
        If (Sayfa3.Cells(a, 4) = "AKSESUAR" Or Sayfa3.Cells(a, 4) = "BANYO" Or Sayfa3.Cells(a, 4) = "DERİ AKSESUAR" Or Sayfa3.Cells(a, 4) = "DOKUMA AKSESUAR" Or Sayfa3.Cells(a, 4) = "KRAVAT/PAPYON" Or Sayfa3.Cells(a, 4) = "SAÇ AKSESUAR" Or Sayfa3.Cells(a, 4) = "STANDART DIŞI" Or Sayfa3.Cells(a, 4) = "TRİKO ÖRME AKSESUAR") Then
                gae = Sayfa3.Cells(a, 3) ' aksesuar gelen
                gaf = gaf + gae ' aksesuar gelen toplamı
                a = a + 1
        ElseIf (Sayfa3.Cells(a, 4) = "OYUNCAK") Then
                hae = Sayfa3.Cells(a, 3) ' oyuncak gelen
                haf = haf + hae ' oyuncak gelen toplamı
                a = a + 1
        ElseIf (Sayfa3.Cells(a, 4) = "ÇORAP") Then
                ıae = Sayfa3.Cells(a, 3) ' çorap gelen
                ıaf = ıaf + ıae ' çorap gelen toplamı
                a = a + 1
        ElseIf (Sayfa3.Cells(a, 4) = "AYAKKABI") Then
                iae = Sayfa3.Cells(a, 3) ' ayakkabı gelen
                iaf = iaf + iae ' ayakkabı gelen toplamı
                a = a + 1
        ElseIf (Sayfa3.Cells(a, 4) = "KOZMETİK" Or Sayfa3.Cells(a, 4) = "TEKSTİL DIŞI") Then
                jae = Sayfa3.Cells(a, 3) ' kozmetik gelen
                jaf = jaf + jae ' kozmetik gelen toplamı
                a = a + 1
        Else
                kae = Sayfa3.Cells(a, 3) ' bu gelen
                kaf = kaf + kae ' bu gelen toplamı
                a = a + 1
         End If
    ElseIf (Sayfa3.Cells(a, 1) = "CK") Then
        If (Sayfa3.Cells(a, 4) = "AKSESUAR" Or Sayfa3.Cells(a, 4) = "BANYO" Or Sayfa3.Cells(a, 4) = "DERİ AKSESUAR" Or Sayfa3.Cells(a, 4) = "DOKUMA AKSESUAR" Or Sayfa3.Cells(a, 4) = "KRAVAT/PAPYON" Or Sayfa3.Cells(a, 4) = "SAÇ AKSESUAR" Or Sayfa3.Cells(a, 4) = "STANDART DIŞI" Or Sayfa3.Cells(a, 4) = "TRİKO ÖRME AKSESUAR") Then
                lae = Sayfa3.Cells(a, 3) ' aksesuar gelen
                laf = laf + lae ' aksesuar gelen toplamı
                a = a + 1
        ElseIf (Sayfa3.Cells(a, 4) = "OYUNCAK") Then
                mae = Sayfa3.Cells(a, 3) ' oyuncak gelen
                maf = maf + mae ' oyuncak gelen toplamı
                a = a + 1
        ElseIf (Sayfa3.Cells(a, 4) = "ÇORAP") Then
                nae = Sayfa3.Cells(a, 3) ' çorap gelen
                naf = naf + nae ' çorap gelen toplamı
                a = a + 1
        ElseIf (Sayfa3.Cells(a, 4) = "AYAKKABI") Then
                If (Sayfa3.Cells(a, 2) = "CK4A ") Then
                    oae = Sayfa3.Cells(a, 3) ' ayakkabı ck4 gelen
                    oaf = oaf + oae ' ayakkabı ck4 gelen toplamı
                    a = a + 1
                Else
                    öae = Sayfa3.Cells(a, 3) ' ayakkabı ck1 gelen
                    öaf = öaf + öae ' ayakkabı ck1 gelen toplamı
                    a = a + 1
                End If
        ElseIf (Sayfa3.Cells(a, 4) = "KOZMETİK" Or Sayfa3.Cells(a, 4) = "TEKSTİL DIŞI") Then
                pae = Sayfa3.Cells(a, 3) ' kozmetik gelen
                paf = paf + pae ' kozmetik gelen toplamı
                a = a + 1
        Else
                rae = Sayfa3.Cells(a, 3) ' ck gelen
                raf = raf + rae ' ck gelen toplamı
                a = a + 1
         End If
    ElseIf (Sayfa3.Cells(a, 1) = "CU") Then
        If (Sayfa3.Cells(a, 4) = "AKSESUAR" Or Sayfa3.Cells(a, 4) = "BANYO" Or Sayfa3.Cells(a, 4) = "DERİ AKSESUAR" Or Sayfa3.Cells(a, 4) = "DOKUMA AKSESUAR" Or Sayfa3.Cells(a, 4) = "KRAVAT/PAPYON" Or Sayfa3.Cells(a, 4) = "SAÇ AKSESUAR" Or Sayfa3.Cells(a, 4) = "STANDART DIŞI" Or Sayfa3.Cells(a, 4) = "TRİKO ÖRME AKSESUAR") Then
                sae = Sayfa3.Cells(a, 3) ' aksesuar gelen
                saf = saf + sae ' aksesuar gelen toplamı
                a = a + 1
        ElseIf (Sayfa3.Cells(a, 4) = "OYUNCAK") Then
                şae = Sayfa3.Cells(a, 3) ' oyuncak gelen
                şaf = şaf + şae ' oyuncak gelen toplamı
                a = a + 1
        ElseIf (Sayfa3.Cells(a, 4) = "ÇORAP") Then
                tae = Sayfa3.Cells(a, 3) ' çorap gelen
                taf = taf + tae ' çorap gelen toplamı
                a = a + 1
        ElseIf (Sayfa3.Cells(a, 4) = "AYAKKABI") Then
                If (Sayfa3.Cells(a, 2) = "CU4A ") Then
                    uae = Sayfa3.Cells(a, 3) ' ayakkabı ck4 gelen
                    uaf = uaf + uae ' ayakkabı ck4 gelen toplamı
                    a = a + 1
                Else
                    üae = Sayfa3.Cells(a, 3) ' ayakkabı ck1 gelen
                    üaf = üaf + üae ' ayakkabı ck1 gelen toplamı
                    a = a + 1
                End If
        ElseIf (Sayfa3.Cells(a, 4) = "KOZMETİK" Or Sayfa3.Cells(a, 4) = "TEKSTİL DIŞI") Then
                vae = Sayfa3.Cells(a, 3) ' kozmetik gelen
                vaf = vaf + vae ' kozmetik gelen toplamı
                a = a + 1
        Else
                yae = Sayfa3.Cells(a, 3) ' cu gelen
                yaf = yaf + yae ' cu gelen toplamı
                a = a + 1
        End If
    Else
        a = a + 1
    End If
Loop
